	<folder>
		<title>Docker</title>
		<snippets>
			<snippet>
				<title>stop containers</title>
				<content>docker stop $(docker ps -q)</content>
			</snippet>
			<snippet>
				<title>stop containers including stopping</title>
				<content>docker stop $(docker ps -a -q)</content>
			</snippet>
			<snippet>
				<title>prune volumes</title>
				<content>docker system prune --volumes</content>
			</snippet>
			<snippet>
				<title>delete images</title>
				<content>docker rmi $(docker images -a -q)</content>
			</snippet>
			<snippet>
				<title>delete containers</title>
				<content>docker rm $(docker ps -a -q)</content>
			</snippet>
			<snippet>
				<title>delete EXITED containers</title>
				<content>docker rm $(docker ps --filter status=exited -q)</content>
			</snippet>
			<snippet>
				<title>標準入力を実行後も開き続け操作</title>
				<content>docker exec -it &lt;コンテナ名&gt; bash</content>
			</snippet>
		</snippets>
